<PhysiCell_settings version="1.4.1">
	<domain>
		<x_min>-1000</x_min>
		<x_max>1000</x_max>
		<y_min>-1000</y_min>
		<y_max>1000</y_max>
		<z_min>-10</z_min>
		<z_max>10</z_max>
		<dx>20</dx>
		<dy>20</dy>
		<dz>20</dz>
		<use_2D>true</use_2D>
	</domain>
	
	<overall>
		<max_time units="min">7200</max_time> 
		<time_units>min</time_units>
		<space_units>micron</space_units>
	</overall>
	
	<parallel>
		<omp_num_threads>4</omp_num_threads>
	</parallel> 
	
	<save>
		<folder>output</folder>  

		<full_data>
			<interval units="min">6</interval>
			<enable>false</enable>
		</full_data>
		 
		<SVG>
			<interval units="min">6</interval>
			<enable>true</enable>
		</SVG>
		
		<legacy_data>
			<enable>false</enable>
		</legacy_data>
	</save>
	
	<user_parameters>
	 
	
        
        <tumor_radius type="double">175</tumor_radius>
        <enable_ecm_outputs type="bool">true</enable_ecm_outputs>
        <initial_density_ecm type="double">0.5</initial_density_ecm>
        <leader_adhesion type="double">2.5</leader_adhesion>
        <leader_repulsion type="double">25</leader_repulsion>
        <follower_adhesion type="double">2.5</follower_adhesion>
        <follower_repulsion type="double">25</follower_repulsion>
        <default_cell_speed type="double">0.05</default_cell_speed>
        <leader_motility_mode type="bool">true</leader_motility_mode>
        <follower_motility_mode type="bool">true</follower_motility_mode>
		
	</user_parameters>
	
</PhysiCell_settings>