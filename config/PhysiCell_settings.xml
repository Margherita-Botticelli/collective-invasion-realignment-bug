<PhysiCell_settings version="1.4.1">
	<domain>
		<x_min>-1000</x_min>
		<x_max>1000</x_max>
		<y_min>-1000</y_min>
		<y_max>1000</y_max>
		<z_min>-10</z_min>
		<z_max>10</z_max>
		<dx>20</dx>
		<dy>20</dy>
		<dz>20</dz>
		<use_2D>true</use_2D>
	</domain>
	
	<overall>
		<max_time units="min" /> 
		<time_units>min</time_units>
		<space_units>micron</space_units>
	</overall>
	
	<parallel>
		<omp_num_threads>8</omp_num_threads>
	</parallel> 
	
	<save>
		<folder>output</folder>  

		<full_data>
			<interval units="min">60</interval>
			<enable>true</enable>
		</full_data>
		 
		<SVG>
			<interval units="min">120</interval>
			<enable>true</enable>
		</SVG>
		
		<legacy_data>
			<enable>false</enable>
		</legacy_data>
	</save>
	
	<user_parameters>
	 
	
		<tumor_radius type="double" />
		<enable_ecm_outputs type="bool" />
		<initial_density_ecm type="double" />
		<leader_adhesion type="double" />
		<follower_adhesion type="double" />
		<default_cell_speed type="double" />
		
	</user_parameters>
	
</PhysiCell_settings>